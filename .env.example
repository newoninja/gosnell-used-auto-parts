# ─────────────────────────────────────────────
# Gosnell Used Auto Parts – Environment Variables
# Copy this file to .env.local and fill in values
# ─────────────────────────────────────────────

# Google Maps Embed API key (optional – free tier works without key)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=

# Formspree endpoint for contact form submissions
# Sign up at https://formspree.io and create a form
NEXT_PUBLIC_FORMSPREE_ENDPOINT=https://formspree.io/f/YOUR_FORM_ID

# EmailJS (alternative to Formspree)
NEXT_PUBLIC_EMAILJS_SERVICE_ID=
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=

# Site URL (used for Open Graph / SEO)
NEXT_PUBLIC_SITE_URL=https://gosnellusedautoparts.com

# Vercel Analytics (auto-detected in Vercel deployments)
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=
